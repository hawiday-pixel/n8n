# n8n API Configuration
# Copy this file to .env and fill in your values
# Get your API key from: n8n Settings → API → Generate API Key

N8N_API_URL=https://your-n8n-instance.hostinger.com/api/v1
N8N_API_KEY=your-api-key-here

# Optional: n8n instance name for reference
N8N_INSTANCE_NAME=production
